<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".activities.DNI_mostrar">

    <ImageView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/dni_placeholder_empty"
            app:layout_constraintStart_toStartOf="@+id/guideline3"
            app:layout_constraintEnd_toEndOf="@id/guideline4"
            android:id="@+id/imageView"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintVertical_bias="0.5"
            tools:ignore="ImageContrastCheck"
            android:importantForAccessibility="no" />

    <androidx.constraintlayout.widget.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideline3"
            app:layout_constraintGuide_begin="21dp"
            android:orientation="vertical" />

    <androidx.constraintlayout.widget.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideline4"
            android:orientation="vertical"
            app:layout_constraintGuide_end="21dp" />

    <TextView
            android:text=""
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/dniApellidoTextView"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="126dp"
            android:layout_marginBottom="66dp"
            app:layout_constraintBottom_toTopOf="@+id/dniNacimientoTextView"
            tools:ignore="SmallSp"
            tools:text="Gustavo"
            style="@style/dniFieldStyle" />

    <TextView
            android:text=""
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/dniApellido2TextView"
            app:layout_constraintStart_toStartOf="@+id/dniApellidoTextView"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/dniApellidoTextView"
            tools:ignore="SmallSp"
            tools:text="Bergel"
            style="@style/dniFieldStyle" />

    <TextView
            android:text=""
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/dniNombreTextView"
            app:layout_constraintStart_toStartOf="@+id/dniApellido2TextView"
            android:layout_marginTop="7dp"
            app:layout_constraintTop_toBottomOf="@+id/dniApellido2TextView"
            tools:ignore="SmallSp"
            tools:text="RamÃ³n"
            style="@style/dniFieldStyle" />

    <TextView
            android:text=""
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/dniNacimientoTextView"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="@+id/dniSexoTextView"
            tools:ignore="SmallSp"
            tools:text="01 01 2001"
            style="@style/dniFieldStyle" />

    <TextView
            android:text=""
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/dniDespTextView"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.527"
            app:layout_constraintStart_toStartOf="@+id/dniNacimientoTextView"
            tools:ignore="SmallSp"
            tools:text="AAAAAAAA"
            style="@style/dniFieldStyle" />

    <TextView
            android:text=""
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/dniValidoFechTextView"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.552"
            app:layout_constraintStart_toStartOf="@+id/dniDespTextView"
            tools:ignore="SmallSp"
            tools:text="01 01 2001"
            style="@style/dniFieldStyle" />

    <TextView
            android:text=""
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/dniNumeroTextView"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.64"
            app:layout_constraintStart_toStartOf="@+id/imageView"
            android:layout_marginStart="9dp"
            tools:ignore="SmallSp"
            style="@style/dniFieldStyle"
            tools:text="99999999-R" />

    <TextView
            android:text=""
            android:layout_width="0dp"
            android:layout_height="10dp"
            android:id="@+id/dniSexoTextView"
            style="@style/dniFieldStyle"
            android:layout_marginTop="20dp"
            android:layout_marginBottom="20dp"
            app:layout_constraintEnd_toEndOf="@+id/dniNombreTextView"
            app:layout_constraintTop_toTopOf="@+id/dniNombreTextView"
            app:layout_constraintStart_toStartOf="@+id/dniNombreTextView"
            app:layout_constraintBottom_toBottomOf="@+id/dniNacimientoTextView"
            tools:ignore="SmallSp,TextSizeCheck"
            tools:text="M" />
</androidx.constraintlayout.widget.ConstraintLayout>